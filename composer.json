{
    "require": {

        "php": "^7.3|^8.0",
        "predis/predis": "^1.0",
        "laravel/lumen-framework": "^8.0", "illuminate/routing": "^8.0", "illuminate/redis": "^8.0", "illuminate/mail": "^8.0", "illuminate/notifications": "^8.0", "laravel/legacy-factories": "^1.0"
    },

    "require-dev": {

        "fakerphp/faker": "^1.9",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.3"
    },

    "autoload": {

        "psr-4": { "App\\": "app/", "Database\\Factories\\": "database/factories/", "Database\\Seeders\\": "database/seeders/" }
    },

    "autoload-dev": {

        "psr-4": { "Tests\\": "tests/" }
    },

    "config": { "preferred-install": "dist", "sort-packages": true, "optimize-autoloader": true },
    "minimum-stability": "dev",
    "prefer-stable": true
}